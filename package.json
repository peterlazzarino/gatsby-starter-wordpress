    "gatsby-plugin-robots-txt": "^1.3.0",
    "gatsby-plugin-sass": "2.0.1",
    "gatsby-plugin-sharp": "2.0.5",
    "gatsby-plugin-sitemap": "2.0.1",
    "gatsby-plugin-styled-components": "3.0.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "2.0.1",
    "gatsby-source-filesystem": "2.0.1",
    "gatsby-transformer-remark": "2.1.3",
    "gatsby-plugin-catch-links": "next",
    "gatsby-plugin-feed": "next",
    "gatsby-plugin-glamor": "next",
    "gatsby-plugin-google-analytics": "next",
