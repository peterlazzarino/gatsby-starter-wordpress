{
  "name": "cv-blog-v2",
  "description": "CollegeVine blog built with gatsbyJS and react.",
  "version": "0.0.1",
  "author": "Peter Lazzarino",
  "dependencies": {
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-styled-components": "^1.5.1",
    "gatsby": "2.0.6",
    "gatsby-image": "2.0.6",
    "gatsby-link": "2.0.1",
